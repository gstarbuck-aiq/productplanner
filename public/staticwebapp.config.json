{
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".json": "application/json",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".ico": "image/x-icon"
  },
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "content-type": "application/javascript"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{css,scss,js,mjs,png,jpg,gif,ico,svg}"]
  }
}